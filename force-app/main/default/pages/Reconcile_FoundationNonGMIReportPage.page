            border-collapse: collapse;
            font-size: 12px;
            padding: 2px;
            border-spacing: 0;
            table-collapse: collapse;
        }
        tr {
            page-break-inside: avoid;
        }
        .rptTbl {
            width: 100%;
        }

        .rptTbl th {
            background-color: #C0C0C0;
        }

        .c5 {
            width: 5%;
        }

        .c3 {
            width: 3%;
        }

        .c55 {
            width: 57%;
        }

        .c57 {
            width: 57%;
        }

        .c37 {
            width: 37%;
        }

        .c54 {
            width: 54%;
        }

        .c10 {
            width: 10%;
        }

        .c30 {
            width: 30%;
        }

        .c17 {
            width: 17%;
        }

        .c11 {
            width: 11%;
        }

        .c40 {
            width: 40%;
        }

        .c20 {
            width: 20%;
        }

        .c50 {
            width: 50%;
        }

        .ar {
            text-align: right;
        }

        .ac {
            text-align: center;
        }

        .bold {
            font-weight: bold;
        }

        .ul {
            text-decoration: underline
        }

        .subHead {
            text-align: center;
            font-weight: bold;
        }

        .big {
            font-size: 16px;
            font-weight: bold;
