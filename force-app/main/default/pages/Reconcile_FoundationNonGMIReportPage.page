    <body>
        <div class="body">
            <apex:variable value="{!1<0 }" var="positive" />
            <apex:variable value="{!1>0}" var="negative" />
            <table class="header">
                <tbody>
                    <tr>
                        <td class="c50 title">ACLU Foundation</td>
                        <td class="c50 ar">Report Refresh: {!todayDate}</td>
                    </tr>
                    <tr>
                        <td>v. Revenue Reconciliation Report</td>
                        <td class="ar">Affiliate: {!affiliate}</td>
                    </tr>
                    <tr>
                        <td>For Affiliate Foundations Without Guaranteed Minimum Incomes</td>
                        <td class="ar">For Period:
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!startDate}" />
                            </apex:outputText> to&nbsp;
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!endDate}" />
                            </apex:outputText>
                        </td>
                    </tr>
                </tbody>
            </table>
            <br/>
            <br/>
            <table class="rptTbl">
                <tbody>
                    <tr>
                        <th class="c3"></th>
                        <th class="c55"></th>
                        <th class="c10"></th>
                        <th class="c30">Notes / Additional Information</th>
                    </tr>
                    <tr>
                        <td class="c3 ar">1.</td>
                        <td class="c55">FOUNDATION BASE RENEWABLE INCOME [Includes Direct Mail Gifts of less than $2,500]</td>
                        <td class="c10 ar"></td>
                        <td class="c30"></td>
                    </tr>
                    <tr>
                        <td class="c3"></td>
                        <td class="c55">a. Gross National Foundation-Banked [N111]</td>
                        <td class="c10 ar">
                            <c:customCurrency curr="{!one_a}" />
                        </td>
                        <td class="c30"></td>
                    </tr>
                </tbody>
            </table>
            <table class="rptTbl">
                <tbody>
                    <tr>
                        <td class="c3"></td>
                        <td class="c40">b. Less Cost Factor</td>
                        <td class="c17">{!foundationCostFactor}% of 1a =</td>
                        <td class="c10 ar">
                            <apex:variable value="{!(one_a * foundationCostFactor * -1)/100 }" var="one_b" />
                            <c:customCurrency curr="{!one_b}" />
                        </td>
                        <td class="c30"></td>
                    </tr>
                </tbody>
            </table>
            <table class="rptTbl">
                <tbody>
                    <tr>
                        <td class="c3"></td>
                        <td class="c55">c. National-Banked Net Workplace Giving</td>
                        <td class="c10 ar">
                            <c:customCurrency curr="{!one_c}" />
                        </td>
                        <td class="c30"></td>
                    </tr>
                    <tr>
                        <td class="c3"></td>
                        <td class="c55">d. Total Net National Foundation-Banked Base Renewable Income [1a+1b+1c]</td>
                        <td class="c10 ar">
                            <apex:variable value="{!one_a + one_b + one_c}" var="one_d" />
                            <c:customCurrency curr="{!one_d}" />
                        </td>
                        <td class="c30"></td>
                    </tr>
                </tbody>
            </table>
            <table class="rptTbl">
                <tbody>
                    <tr style="border-bottom: 2px solid black;">
