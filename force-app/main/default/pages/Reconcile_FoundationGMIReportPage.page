<apex:page Controller="Reconcile_FoundationGMIReportController" renderAs="pdf" applyHtmlTag="false" applyBodyTag="false" showHeader="false" sidebar="false">

<html>
    <head>
        <title>Foundation GMI</title>
        <style type="text/css">
        .body{
          width: 700px;
          display: block;
          font-size: 12px;
          text-align: left;
        }
        .beforebreak {
            page-break-before: always;
        }
        .title{
          text-decoration: underline;
          font: bold;
          font-size: 20px;
          padding: 0 0 5px 0;
        }
        .header{
            width: 100%;
            border: 0;      }
        .header td{
            border: 0;
            vertical-align: bottom;
        }
        .header tr{
            border: 0;
        }       
        table,tr,th,td{
            border:1px solid #C0C0C0;
          border-collapse:collapse;
            font-size: 12px;
            padding: 2px;
            border-spacing: 0;
          table-collapse: collapse;
        }
        tr {
            page-break-inside: avoid;
        }
        .rptTbl{
          width: 100%;
        }
        .rptTbl th{
            background-color: #C0C0C0;
        }
        .c5{width: 5%;}
        .c3{width: 3%;}
        .c55{width: 57%;}
        .c57{width: 57%;}
        .c37{width: 37%;}
        .c54{width: 54%;}
        .c10{width: 10%;}
        .c30{width: 30%;}
        .c17{width: 17%;}
        .c11{width: 11%;}
        .c40{width: 40%;}
        .c20{width: 20%;}
        .c50{width: 50%;}
        .ar{text-align:right;}
        .ac{text-align:center;}
        .bold{font-weight: bold;}
        .ul{text-decoration: underline}
        .subHead{
            text-align:center;
            font-weight: bold;
        }
        .big{
            font-size: 16px;
            font-weight: bold;
        }
        </style>
</head>

<body>
<div class="body">
  <table class="header" >
    <tbody>
        <tr>
            <td class="c50 title">ACLU Foundation</td>
            <td class="c50 ar">Report Refresh: {!todayDate}</td>
        </tr>
        <tr>
            <td>v. Revenue Reconciliation Report</td>
            <td class="ar">Affiliate: {!affiliate}</td>
        </tr>
        <tr>
            <td>For Affiliate Foundations With Guaranteed Minimum Incomes</td>
            <td class="ar">For Period: 
                <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                    <apex:param value="{!startDate}" />
                </apex:outputText> to&nbsp; 
                <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                    <apex:param value="{!endDate}" />  
                </apex:outputText>
            </td>
        </tr>
    </tbody>
  </table>
  <br/>
  <br/>


    <table class="rptTbl"><tbody>
        <tr>
            <th class="c3"></th>
            <th class="c55"></th>
            <th class="c10"></th>
            <th class="c30">Notes / Additional Information</th>
        </tr>
        <tr>
            <td class="c3 ar">1.</td>
            <td class="c55">FOUNDATION BASE RENEWABLE INCOME [Includes Direct Mail Gifts of less than $2,500]</td>
            <td class="c10 ar"></td>
            <td class="c30"></td>
        </tr>
        <tr>
            <td class="c3"></td>
            <td class="c55">a. Gross National Foundation-Banked</td>
            <td class="c10 ar">
              <c:customCurrency curr="{!one_a}" />
            </td>
            <td class="c30"></td>
        </tr>
    </tbody></table>
    <table class="rptTbl" ><tbody>
        <tr>
            <td class="c3"></td>
            <td class="c40">b. Less Cost Factor</td>
            <td class="c17">{!foundationCostFactor}% of 1a =</td>
            <td class="c10 ar">
              <apex:variable value="{!one_a * foundationCostFactor/100*-1 }" var="one_b"/>
              <c:customCurrency curr="{!one_b}" />
            </td>
            <td class="c30"></td>
        </tr>
    </tbody></table>
    <table class="rptTbl"><tbody>
        <tr>
            <td class="c3"></td>
            <td class="c55">c. National-Banked Net Workplace Giving</td>
            <td class="c10 ar">
              <c:customCurrency curr="{!one_c}" />
            </td>
            <td class="c30"></td>
        </tr>
        <tr>
            <td class="c3"></td>
            <td class="c55">d. Total Net National Foundation-Banked Base Renewable Income [1a+1b+1c]</td>
            <td class="c10 ar">
                <apex:variable value="{!one_a + one_b+ one_c}" var="one_d"/>
                <c:customCurrency curr="{!one_d}" />
            </td>
            <td class="c30"></td>
        </tr>
    </tbody></table>
    <table class="rptTbl" ><tbody>    
        <tr style="border-bottom: 2px solid black;">
            <td class="c3"></td>
            <td class="c40">e. Affiliate Foundation Share of Net National Foundation BRI</td>
            <td class="c17">74.00% of 1d =</td>
            <td class="c10 ar">
                <apex:variable value="{!one_d * 0.74}" var="one_e"/>
                <c:customCurrency curr="{!one_e}" />
            </td>
            <td class="c30"></td>
        </tr>
    </tbody></table>
    <table class="rptTbl" ><tbody>    
        <tr style="border-bottom: 2px solid black;">
            <td class="c3"></td>
            <td class="c55">f. Total Net Affiliate Foundation-Banked Base Renewable Income</td>
            <td class="c10 ar">
                <apex:variable value="{!a_1a - a_1b}" var="one_f"/>
                <c:customCurrency curr="{!one_f}" />
            </td>
            <td class="c30"></td>
        </tr>
    </tbody></table>
    <table class="rptTbl" ><tbody>    
        <tr style="border-bottom: 2px solid black;">
            <td class="c3"></td>
            <td class="c40">g. National Foundation Share of Net Affiliate Foundation BRI</td>
            <td class="c17">26.00% of 1f =</td>
            <td class="c10 ar">
                <apex:variable value="{!one_f * 0.26 *-1}" var="one_g"/>
                <c:customCurrency curr="{!one_g}" />
            </td>
            <td class="c30"></td>
        </tr>
    </tbody></table>
    <table class="rptTbl" ><tbody>
